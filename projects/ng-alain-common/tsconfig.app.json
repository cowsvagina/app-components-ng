{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "paths": {
    "@app/*": ["src/app/*"],
    "@core": ["src/app/core/index"],
    "@core/*": ["src/app/core/*"],
    "@shared": ["src/app/shared/index"],
    "@env/*": ["src/environments/*"]
  }
}
